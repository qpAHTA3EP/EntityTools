<?xml version="1.0" encoding="utf-8"?>
<ProfilePreprocessor xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Active>true</Active>
  <AutoSave>false</AutoSave>
  <Replacement>
    <ReplacementItem>
      <Type>Regex</Type>
      <Pattern>\bAdventuring(?!Trousers|Rings|Hands)(?&lt;bagName&gt;\w+\b)</Pattern>
      <Replacement>${bagName}</Replacement>
    </ReplacementItem>
    <ReplacementItem>
      <Type>Simple</Type>
      <Pattern>AdventuringHands</Pattern>
      <Replacement>Melee</Replacement>
    </ReplacementItem>
    <ReplacementItem>
      <Type>Simple</Type>
      <Pattern>AdventuringTrousers</Pattern>
      <Replacement>Pants</Replacement>
    </ReplacementItem>
    <ReplacementItem>
      <Type>Simple</Type>
      <Pattern>AdventuringRings</Pattern>
      <Replacement>Ring</Replacement>
    </ReplacementItem>
  </Replacement>
</ProfilePreprocessor>